{
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "Logs/zemisapi-{Hour}.txt",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} | {Level} | {SourceContext} | {Message} | {Exception} {NewLine}"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "MySql": "Server=db;Database=zemis;Uid=root;Pwd=root"
  },
  "Mysql": {
    "Version": "8.0.25"
  }
}
