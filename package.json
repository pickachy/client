{
  "name": "pickachy",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve --host=0.0.0.0 --port 4201 --disable-host-check",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "dev:ssr": "ng run pickachy:serve-ssr",
    "serve:ssr": "node dist/pickachy/server/main.js",
    "build:ssr": "ng build && ng run pickachy:server && npm run compress",
    "prerender": "node --max-old-space-size=1500 ./node_modules/@angular/cli/bin/ng run pickachy:prerender && npm run compress",
    "compress": "gzipper compress --gzip --brotli --gzip-level=9 --remove-larger --exclude=woff2  dist/pickachy/browser",
    "source-map-explorer:build": "ng build --source-map",
    "source-map-explorer:show": "./node_modules/.bin/source-map-explorer dist/pickachy/browser/*",
    "source-map-explorer:start": "npm run source-map-explorer:build && npm run source-map-explorer:show"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.0.1",
    "@angular/cdk": "14.0.1",
    "@angular/common": "14.0.1",
    "@angular/compiler": "14.0.1",
    "@angular/core": "14.0.1",
    "@angular/forms": "14.0.1",
    "@angular/platform-browser": "14.0.1",
    "@angular/platform-browser-dynamic": "14.0.1",
    "@angular/platform-server": "14.0.1",
    "@angular/router": "14.0.1",
    "@angular/service-worker": "14.0.1",
    "@apollo/client": "3.6.8",
    "@ng-select/ng-select": "9.0.1",
    "@nguniversal/express-engine": "14.0.0",
    "ngx-progressbar": "9.0.0",
    "apollo-angular": "3.0.1",
    "express": "4.18.1",
    "graphql": "16.5.0",
    "lazysizes": "5.3.2",
    "ngx-toastr": "14.3.0",
    "rxjs": "7.5.5",
    "tslib": "2.4.0",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "14.0.0",
    "@angular-devkit/architect": "0.1400.1",
    "@angular-devkit/build-angular": "14.0.1",
    "@angular-eslint/builder": "13.5.0",
    "@angular-eslint/eslint-plugin": "13.5.0",
    "@angular-eslint/eslint-plugin-template": "13.5.0",
    "@angular-eslint/schematics": "13.5.0",
    "@angular-eslint/template-parser": "13.5.0",
    "@angular/cli": "13.3.7",
    "@angular/compiler-cli": "14.0.1",
    "@nguniversal/builders": "13.1.1",
    "@types/express": "4.17.13",
    "@types/node": "14.18.12",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "eslint": "8.17.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.3.2",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-unused-imports": "2.0.0",
    "eslint-webpack-plugin": "3.1.1",
    "gzipper": "7.1.0",
    "ngx-unused-css": "4.0.0-1",
    "prettier": "2.7.0",
    "source-map-explorer": "2.5.2",
    "typescript": "4.7.3"
  }
}
