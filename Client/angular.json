{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "zemis": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist/zemis/browser",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets/images",
              "src/manifest.webmanifest",
              { "glob": "robots.txt", "input": "src/assets/", "output": "/" }
            ],
            "styles": ["src/styles.scss"],
            "sourceMap": false,
            "scripts": [],
            "customWebpackConfig": {
              "path": "./webpack.config.js"
            },
            "serviceWorker": true,
            "ngswConfigPath": "ngsw-config.json"
          },
          "configurations": {
            "production": {
              "optimization": {
                "styles": {
                  "inlineCritical": false
                }
              },
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "4mb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "5kb"
                }
              ]
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "zemis:build"
          },
          "configurations": {
            "development": {
              "browserTarget": "zemis:build:development"
            },
            "production": {
              "browserTarget": "zemis:build:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
          }
        },
        "server": {
          "builder": "@angular-builders/custom-webpack:server",
          "options": {
            "outputPath": "dist/zemis/server",
            "main": "server.ts",
            "tsConfig": "tsconfig.server.json",
            "inlineStyleLanguage": "scss"
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "optimization": false,
              "sourceMap": true,
              "extractLicenses": false
            }
          },
          "defaultConfiguration": "production"
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "options": {
            "browserTarget": "zemis:build",
            "serverTarget": "zemis:server"
          },
          "configurations": {
            "development": {
              "browserTarget": "zemis:build:development",
              "serverTarget": "zemis:server:development"
            },
            "production": {
              "browserTarget": "zemis:build:production",
              "serverTarget": "zemis:server:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "browserTarget": "zemis:build:production",
            "serverTarget": "zemis:server:production",
            "routes": [
              "/",
              "loans/mfo/1",
              "loans/mfo/2",
              "loans/mfo/3",
              "loans/mfo/4",
              "loans/mfo/5",
              "loans/mfo/6",
              "loans/mfo/7",
              "loans/mfo/8",
              "loans/mfo/9",
              "loans/mfo/10",
              "loans/mfo/11",
              "loans/mfo/12",
              "loans/mfo/13",
              "loans/mfo/14",
              "loans/mfo/15",
              "loans/mfo/16",
              "loans/mfo/17",
              "loans/mfo/18",
              "loans/mfo/19",
              "loans/mfo/20",
              "loans/mfo/21",
              "loans/mfo/22",
              "loans/mfo/23",
              "loans/mfo/24",
              "loans/mfo/25",
              "loans/mfo/26",
              "loans/mfo/27",
              "loans/mfo/28",
              "loans/mfo/29"
            ]
          },
          "configurations": {
            "production": {}
          }
        }
      }
    }
  },
  "defaultProject": "zemis",
  "cli": {
    "defaultCollection": "@angular-eslint/schematics"
  }
}
